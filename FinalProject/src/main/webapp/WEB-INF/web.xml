<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ClientAuthentication</servlet-name>
  	<display-name>ClientAuthentication</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.ClientAuthentication</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClientRegister</servlet-name>
  	<display-name>ClientRegister</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.ClientRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>userLogout</servlet-name>
  	<display-name>userLogout</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.userLogout</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProblemDetector</servlet-name>
  	<display-name>ProblemDetector</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.ProblemDetector</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DoctorAuthenication</servlet-name>
  	<display-name>DoctorAuthenication</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.DoctorAuthenication</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DoctorRegister</servlet-name>
  	<display-name>DoctorRegister</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.DoctorRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProcessSlots</servlet-name>
  	<display-name>ProcessSlots</display-name>
  	<description></description>
  	<servlet-class>com.rishikesh.ProcessSlots</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClientAuthentication</servlet-name>
  	<url-pattern>/client_auth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClientRegister</servlet-name>
  	<url-pattern>/client_register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>userLogout</servlet-name>
  	<url-pattern>/userLogout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProblemDetector</servlet-name>
  	<url-pattern>/problem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DoctorAuthenication</servlet-name>
  	<url-pattern>/doctor_auth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DoctorRegister</servlet-name>
  	<url-pattern>/doctor_register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProcessSlots</servlet-name>
  	<url-pattern>/process_slots</url-pattern>
  </servlet-mapping>
</web-app>
